Algoritmo "execicio 3"

Var
   idade, apo:inteiro
   nome:caractere


Inicio
      escreva ("digite seu nome")
      leia (nome)
      escreval("digite seu idade")
      leia (idade)
      apo<-65-idade
      se (idade<=18)entao
      escreva("Você ainda é muito novo!")
      senao
      escreva(nome,"ainda faltam ", apo , "anos para você se aposentar!")
      fimse

      

Fimalgoritmo
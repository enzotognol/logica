Algoritmo "10"

Var
 aux,n1,n2: inteiro



Inicio
   escreva("informe um vlor")
   leia(n1)
   escreva("informe outro vlor")
   leia(n2)
   escreval("")
   escreval("o valor do primero numero é",n1)
   escreval("o valor do segundo numero é",n2)
   aux <-n1
   n1<-n2
   n2<-aux
   escreval("")
   escreval("o valor do primero numero é",n1)
   escreval("o valor do segundo numero é",n2)

Fimalgoritmo
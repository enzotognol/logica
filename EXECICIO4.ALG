Algoritmo "execicio4"

var
   nome:caracter
   vendas,meta:real

Inicio
      escreva("digite o nome do fucionario")
      leia (nome)
      escreval("digite quantas vendas o funcionario fez")
      leia (vendas)
      meta <- 200
      se (vendas >=200) entao
      escreval("ganhou")
      senao
      escreval("nao ganhou")
      fimse

Fimalgoritmo